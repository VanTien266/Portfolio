{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "quotes": [
      "warn",
      "single"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-restricted-exports": [
      "off"
    ],
    "class-methods-use-this": [
      "off"
    ],
    "import/export": 0,
    "no-unused-vars": [
      "warn"
    ],
    "react/prop-types": [
      "off"
    ]
  }
}